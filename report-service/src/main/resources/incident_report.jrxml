<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
              http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="incident_report" pageWidth="595" pageHeight="842">

    <parameter name="incidentId" class="java.lang.Long"/>
    <parameter name="LOGO_PATH" class="java.lang.String"/>

    <queryString>
        <![CDATA[]]>
    </queryString>

    <title>
        <band height="100">
            <image>
                <reportElement x="0" y="0" width="100" height="50"/>
                <imageExpression><![CDATA[$P{LOGO_PATH}]]></imageExpression>
            </image>
            <staticText>
                <reportElement x="120" y="20" width="300" height="30"/>
                <text><![CDATA[Rapport d'Incident]]></text>
            </staticText>
        </band>
    </title>

    <detail>
        <band height="50">
            <textField>
                <reportElement x="0" y="0" width="500" height="20"/>
                <textFieldExpression>
                    <![CDATA["Incident ID: " + $P{incidentId}]]>
                </textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>